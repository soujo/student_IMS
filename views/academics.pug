<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Academics</title>
    <link rel="stylesheet" href="../static/css/academics.css">
</head>

<body>
    <div class="container">
        include ./partials/header.pug
        <div class="aside__section">
            include ./partials/aside.pug
            <section>
                include ./partials/flag.pug
                -if(msg.length>0){
                    .success #{msg}
                -}
                <br><br><br>
                <div class="secondary__details">
                    <a href="/student/academicsEdit">
                        <div class="edit">Edit</div>
                    </a>
                    <table>
                        <thead>
                            <tr>
                                <th>Institute : 10th (Matric) Details</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th>Degree Name</th>
                                <td>#{secondaryDegreeName}</td>
                                <th>Board Name</th>
                                <td>#{secondaryBoardName}</td>
                            </tr>
                            <tr>
                                <th>Institute Name</th>
                                <td>#{secondaryInstituteName}</td>
                                <th>Passing Year</th>
                                <td>#{secondaryPassingYear}</td>
                            </tr>
                            <tr>
                                <th>Obtained Marks</th>
                                <td>#{secondaryObtainedMarks}</td>
                                <th>Total Marks</th>
                                <td>#{secondaryTotalMarks}</td>
                            </tr>
                            <tr>
                                <th>Total CGPA</th>
                                <td>#{secondaryTotalCGPA}</td>
                                <th>Total Percentage</th>
                                <td>#{secondaryTotalPercentage}</td>
                            </tr>
                            <tr>
                                <th>Division</th>
                                <td>#{secondaryDivision}</td>
                                <th>Specialization</th>
                                <td>#{secondarySpecialization}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <br><br><br>
                <div class="senior__secondary__details">
                    <table>
                        <thead>
                            <tr>
                                <th>Last Institute : +2 (Intermediate) Details</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th>Degree Name</th>
                                <td>#{seniorSecondaryDegreeName}</td>
                                <th>Board Name</th>
                                <td>#{seniorSecondaryBoardName}</td>
                            </tr>
                            <tr>
                                <th>Institute Name</th>
                                <td>#{seniorSecondaryInstituteName}</td>
                                <th>Passing Year</th>
                                <td>#{seniorSecondaryPassingYear}</td>
                            </tr>
                            <tr>
                                <th>Obtained Marks</th>
                                <td>#{seniorSecondaryObtainedMarks}</td>
                                <th>Total Marks</th>
                                <td>#{seniorSecondaryTotalMarks}</td>
                            </tr>
                            <tr>
                                <th>Total CGPA</th>
                                <td>#{seniorSecondaryTotalCGPA}</td>
                                <th>Total Percentage</th>
                                <td>#{seniorSecondaryTotalPercentage}</td>
                            </tr>
                            <tr>
                                <th>Division</th>
                                <td>#{seniorSecondaryDivision}</td>
                                <th>Specialization</th>
                                <td>#{seniorSecondarySpecialization}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <br><br><br>
                <div class="institute__details">
                    <table>
                        <thead>
                            <tr>
                                <th>Current Institute : +2+3/4 (Graduation) Details</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th>Degree Name</th>
                                <td>B.Tech</td>
                                <th>University Name</th>
                                <td>MAKAUT</td>
                            </tr>
                            <tr>
                                <th>Institute Name</th>
                                <td>Kalyani Goverment Engineering College</td>
                                <th>Passing Year</th>
                                <td>#{collegePassingYear}</td>
                            </tr>
                            <tr>
                                <th>Obtained Marks</th>
                                <td>#{collegeObtainedMarks}</td>
                                <th>Total Marks</th>
                                <td>#{collegeTotalMarks}</td>
                            </tr>
                            <tr>
                                <th>Total CGPA</th>
                                <td>#{collegeTotalCGPA}</td>
                                <th>Total Percentage</th>
                                <td>#{collegeTotalPercentage}</td>
                            </tr>
                            <tr>
                                <th>Division</th>
                                <td>#{collegeDivision}</td>
                                <th>Department</th>
                                <td>#{branch}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <br><br>
            </section>
        </div>
        include ./partials/footer.pug
    </div>
    <script src="../static/js/academics.js"></script>
</body>

</html>